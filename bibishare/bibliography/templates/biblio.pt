<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<p metal:use-macro="main.macros['master']" i18n:domain="bibishare">
    <div class="middle" metal:fill-slot="middle">
        <div class="breadcrumb">
            
        </div>
        <div class="leftCol">
            &#160;
        </div>
        <a tal:condition="user" href="${request.route_path('bibliography.edit_entry', id=request.matchdict['id'])}">Edit this article</a>

        <h3 class="biblio_title" tal:content="bibitex.title">You are viewing my article</h3>
      
        <dl class="metaddata">

            <dt>Author:</dt>
            <dd tal:repeat="author bibitex.authors"><span tal:replace="author">Soares, Sandra Regina</span></dd>
            
            <dt tal:condition="'pages' in bibitex">pages:</dt>
            <dd tal:condition="'pages' in bibitex" tal:content="bibitex.pages">12</dd>

            <dt tal:condition="'year' in bibitex">Year of Publication:</dt>
            <dd tal:condition="'year' in bibitex" tal:content="bibitex.year">2011</dd>
            
            <dt tal:condition="'publisher' in bibitex">Publisher:</dt>
            <dd tal:condition="'publisher' in bibitex" tal:content="bibitex.publisher">Bireme Press</dd>
    
            <dt tal:condition="'keywords' in bibitex">Keywords:</dt>
            <dd tal:condition="'keywords' in bibitex" tal:content="bibitex.keywords">C++, languages, programming</dd>

        </dl>

        <div tal:condition="download" class="document_download">
            <a class="download" href="${'%s/download/%s' % (request.url,download)}">Download</a>
        </div>

        <div class="wiki">
          <span tal:replace="structure wiki" />
        </div>
        <div class="bibitex">
          <span tal:replace="structure reference" />
        </div>

        <div class="spacer"></div>
    </div>
</p>