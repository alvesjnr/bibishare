<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<p metal:use-macro="main.macros['master']" i18n:domain="bibishare">
    <div class="middle" metal:fill-slot="middle">
        <div class="breadcrumb">
            
        </div>
        <div class="leftCol">
            &#160;
        </div>
        <a href="${request.route_path('bibliography.edit_entry', id=request.matchdict['id'])}">Edit this article</a>

        <h3 tal:content="bibitex.title">You are viewing my article</h3>
        
        <dl class="metaddata">
            <dt>Author:</dt>
            <dd tal:repeat="author bibitex.authors"><span tal:replace="author">Soares, Sandra Regina</span></dd>

        </dl>
        <div class="wiki">
          <span tal:replace="structure wiki" />
        </div>
        <div class="bibitex">
          <span tal:replace="structure reference" />
        </div>

        <div class="spacer"></div>
    </div>
</p>